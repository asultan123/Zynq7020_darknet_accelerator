alias dew = dashboard expression watch
alias deu = dashboard expression unwatch
alias dec = dashboard expression clear
alias sh = shell
# auto generated by GDB dashboard

dashboard -layout !assembly expressions history !memory !registers source stack !threads
dashboard source -style context 20
dashboard stack -style compact True
dashboard expressions -output /dev/ttys002
dashboard source -output /dev/ttys004
dashboard stack -output /dev/ttys001
